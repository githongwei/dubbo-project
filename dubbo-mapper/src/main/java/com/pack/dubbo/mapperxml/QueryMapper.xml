<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pack.dubbo.mapper.QueryMapper">
    <select id="selectName" parameterType="com.pack.dubbo.entity.User" resultType="String">
        SELECT u.password FROM user u WHERE u.name = #{name };
    </select>
</mapper>